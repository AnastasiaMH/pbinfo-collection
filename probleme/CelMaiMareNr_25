int cmmnr(int n)
{
    int v[10]={}, cif, nr=0;
    while (n)
    {
        cif=n%10;
        v[cif]++;
        n/=10;
    }
    for (int i=9; i>=0; i--)
    {
        for (int j=1; j<=v[i]; j++)
            nr=nr*10+i;
    }
    return nr;
}
